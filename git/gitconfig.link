[user]
	name = Martin Hanzel
	email = arthanzel@gmail.com
[core]
	excludesfile = /Users/martin/.gitignore_global
	editor = nano
[alias]
	br = branch
	s = status -sb
	st = status
	hist = log --pretty=format:"%h %s (%an, %ar)"
	unstage = reset HEAD --
	tip = log -1 HEAD
[url "https://"]
	insteadOf = git://
