[user]
	name = Martin Hanzel
	email = arthanzel@gmail.com
[core]
	excludesfile = /Users/martin/.gitignore_global
	editor = nano
[alias]
	a = add .
	br = branch
	cm = commit -m
	s = status -sb
	st = status
	tip = log -1 HEAD
	unstage = reset HEAD --

# uOttawa blocks the git protocol on the wifi, inconveniently
[url "https://"]
	insteadOf = git://
