[user]
	name = Martin Hanzel
	email = arthanzel@gmail.com
[core]
	excludesfile = /Users/martin/.gitignore_global
	editor = nano
[alias]
	a = add .
	br = branch
	cm = commit -m
	hist = log --pretty=format:"%h %s (%an, %ar)"
	s = status -sb
	st = status
	tip = log -1 HEAD
	unstage = reset HEAD --
[url "https://"]
	insteadOf = git://
